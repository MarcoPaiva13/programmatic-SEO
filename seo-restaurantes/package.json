    "next-seo": "^5.15.0",
    "nprogress": "^0.2.0",
    "preact": "^10.15.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^4.0.11",
    "react-leaflet": "^4.2.1",
    "sharp": "^0.33.1",
    "swr": "^2.2.0",
    "web-vitals": "^3.3.2"
  },
  "devDependencies": {
    "@lhci/cli": "^0.12.0",
